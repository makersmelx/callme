{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-console": 0,
    "import/prefer-default-export": 0
  }
}
